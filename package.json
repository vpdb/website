		"rollbar-sourcemap-webpack-plugin": "2.4.0",
		"stream-to-array": "2.3.0",
		"style-loader": "0.23.0",
		"stylus": "0.54.5",
		"stylus-loader": "3.0.2",
		"svg-sprite-loader": "4.1.3",
		"svgo": "1.1.1",
